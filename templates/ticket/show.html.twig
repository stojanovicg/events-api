<!DOCTYPE html>

<title>Ticket</title>

{% block body %}
    <h1>Ticket</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ ticket.id }}</td>
            </tr>
            <tr>
                <th>TicketType</th>
                <td>{{ ticket.ticketType }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ ticket.price }}</td>
            </tr>
            <tr>
                <th>QuantityAvailable</th>
                <td>{{ ticket.quantityAvailable }}</td>
            </tr>
            <tr>
                <th>IsActive</th>
                <td>{{ ticket.isActive ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_ticket_index') }}">back to list</a>

    <a href="{{ path('app_ticket_edit', {'id': ticket.id}) }}">edit</a>

    {{ include('ticket/_delete_form.html.twig') }}
{% endblock %}
